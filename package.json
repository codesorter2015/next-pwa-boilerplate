{
  "name": "next-pwa-static",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "isomorphic-unfetch": "^2.0.0",
    "next": "^5.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "express": "^4.16.3",
    "flow-bin": "^0.67.1",
    "netlify-cli": "^1.2.2",
    "serve": "^6.5.2",
    "workbox-cli": "^3.0.0"
  },
  "scripts": {
    "server:dev": "node src/server",
    "server:prod": "node src/server",
    "build:sw": "workbox generateSW workbox-config.js",
    "build:static":
      "next build src/client && next export src/client && npm run build:sw",
    "server:static-local": "npm run build:static && serve src/client/out",
    "deploy:static": "npm run build:static && netlify deploy -p src/client/out"
  }
}
